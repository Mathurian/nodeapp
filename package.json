{
  "name": "event-manager-backend",
  "version": "1.0.0",
  "description": "Event Manager Contest System - Node.js Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:db:setup": "bash scripts/test-db-setup.sh",
    "test:db:teardown": "bash scripts/test-db-teardown.sh",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "prisma:generate": "prisma generate",
    "load:smoke": "k6 run tests/load/smoke-test.js",
    "load:test": "k6 run tests/load/load-test.js",
    "load:stress": "k6 run tests/load/stress-test.js",
    "load:all": "npm run load:smoke && npm run load:test",
    "perf:analyze": "node scripts/analyze-performance.js",
    "perf:indexes": "node scripts/verify-indexes.js",
    "cache:clear": "redis-cli FLUSHDB",
    "cache:stats": "redis-cli INFO stats",
    "cache:monitor": "redis-cli MONITOR",
    "db:optimize": "npx prisma migrate deploy && node scripts/analyze-queries.js",
    "db:indexes": "psql $DATABASE_URL -f scripts/verify-indexes.sql",
    "db:stats": "psql $DATABASE_URL -c 'SELECT schemaname, tablename, pg_size_pretty(pg_total_relation_size(schemaname||''.''||tablename)) AS size FROM pg_tables WHERE schemaname = ''public'' ORDER BY pg_total_relation_size(schemaname||''.''||tablename) DESC;'",
    "deploy:check": "npm run test && npm run load:smoke && npm run build",
    "deploy:prod": "npm run deploy:check && pm2 restart event-manager-api",
    "health:check": "curl http://localhost:3000/health",
    "health:db": "psql $DATABASE_URL -c 'SELECT 1;'",
    "health:redis": "redis-cli ping"
  },
  "keywords": [
    "event-management",
    "contest-system",
    "scoring",
    "judging"
  ],
  "author": "Event Manager Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@azure/storage-blob": "^12.29.1",
    "@google-cloud/storage": "^7.17.3",
    "@prisma/client": "^5.19.1",
    "@sentry/node": "^10.25.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/handlebars": "^4.0.40",
    "@types/node-cache": "^4.1.3",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/ssh2-sftp-client": "^9.0.6",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.63.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "playwright": "^1.40.0",
    "prisma": "^5.19.1",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.15.0",
    "qrcode": "^1.5.4",
    "react-markdown": "^10.1.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.33.0",
